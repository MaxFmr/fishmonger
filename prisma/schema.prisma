generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}




model MvtTracabapp {

  id Int     @id @default(autoincrement())
  id2 String @unique
  date String
  heure String
  acheteur  String
  criee String
  numLot String
  faoCodeEspece String
  especeNomCommun String
  especeNomScientifique String
  calibre String
  codePresentation String
  codeFraicheur String
  poidsLot  Float
  typeBac String
  nombreBacs Int
  prixKg Float
  codeTransaction String
  montantLot Float
  codeEngin String
  zonePeche String
  codeCfrNavire String
  marquageExtNavire String
  nomNavire String
  
}



